{
  "type": "module",
  "productName": "enjoy-next",
  "name": "enjoy-next",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate -- -d src/main/db/datasource.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/main/db/datasource.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/main/db/datasource.ts"
  },
  "keywords": [],
  "author": {
    "name": "an-lee",
    "email": "an.lee.work@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron-forge/plugin-vite": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/vite": "^4.1.0",
    "@tanstack/react-router": "^1.114.32",
    "@tanstack/react-router-devtools": "^1.114.32",
    "@tanstack/router-plugin": "^1.114.32",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.30",
    "@types/react": "^19.0.13",
    "@types/react-dom": "^19.0.5",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "35.1.2",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "fs-extra": "^11.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reflect-metadata": "^0.2.2",
    "tailwindcss": "^4.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.21",
